{
  "name": "rudderstack-e2e-cypress",
  "version": "1.0.0",
  "description": "End-to-end automation framework for RudderStack using Cypress and Cucumber",
  "main": "index.js",
  "scripts": {
  "cy:open": "cross-env DOTENV_CONFIG_PATH=.env.dev cypress open",
  "test:dev": "cross-env DOTENV_CONFIG_PATH=.env.dev cypress run",
  "test:qa":  "cross-env DOTENV_CONFIG_PATH=.env.qa  cypress run",
  "test:prod":"cross-env DOTENV_CONFIG_PATH=.env.prod cypress run",
  "test:ci": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=true,json=true"
},
  "keywords": [
    "cypress",
    "cucumber",
    "rudderstack",
    "e2e",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cross-env": "^10.0.0",
    "cypress": "^13.6.2",
    "dotenv": "^16.3.1"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/step_definitions/",
    "nonGlobalStepDefinitions": false,
    "filterSpecs": true,
    "omitFiltered": true
  }
}

